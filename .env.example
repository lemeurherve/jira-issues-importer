# Example .env configuration for Jira to GitHub migration
# Copy this to .env and fill in your actual values

# Required: Jira Configuration
export JIRA_MIGRATION_JIRA_PROJECT_NAME="JENKINS"
export JIRA_MIGRATION_JIRA_URL="https://issues.jenkins.io"
export JIRA_MIGRATION_JIRA_USER="your-jira-username"
export JIRA_MIGRATION_JIRA_TOKEN="your-jira-api-token"
export JIRA_MIGRATION_JIRA_DONE_ID="3"

# Required: GitHub Configuration
export JIRA_MIGRATION_GITHUB_NAME="jenkinsci"
export JIRA_MIGRATION_GITHUB_ACCESS_TOKEN="your-github-pat"

# Optional: Advanced Configuration
export JIRA_MIGRATION_HOSTED_ARTIFACT_ORG_REPO="jenkinsci/artifacts-from-jira-issues"
export JIRA_MIGRATION_REDIRECTION_SERVICE="https://issue-redirect.jenkins.io"
export JIRA_MIGRATION_INCLUDE_COMPONENT_IN_LABELS="true"

# Optional: Jira Commenter Configuration
export IS_USER_JIRA_ADMIN="false"
export DONT_NOTIFY_JIRA_USERS="false"
export ARCHIVE_JIRA_ISSUES="false"
export JIRA_MIGRATION_JIRA_PROJECT_DESC="Jenkins"
export EXPORTED_LABEL="github-import-done"
export COMMENTS_FILE="jira-comments.txt"

# Optional: Watchers and Remote Links
export JIRA_MIGRATION_PARALLEL_COUNT="100"

# Optional: JQL Configuration (will be overridden by orchestration script)
# export JIRA_MIGRATION_JQL_MAX_RESULTS="1000"
